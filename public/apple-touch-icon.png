  <rect x="40" y="115" width="85" height="8" rx="4" fill="white" opacity="0.9"/>
  
  <!-- AI/correlation lines -->
  <path d="M30 35 L150 145" stroke="white" stroke-width="4" opacity="0.3" stroke-linecap="round"/>
