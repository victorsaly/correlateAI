<svg width="180" height="180" viewBox="0 0 180 180" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background with rounded corners for iOS -->
  <rect x="0" y="0" width="180" height="180" rx="40" fill="url(#gradient)"/>
  
  <!-- Database/correlation icon scaled up -->
  <rect x="40" y="55" width="100" height="10" rx="5" fill="white" opacity="0.95"/>
  <rect x="40" y="70" width="80" height="8" rx="4" fill="white" opacity="0.8"/>
